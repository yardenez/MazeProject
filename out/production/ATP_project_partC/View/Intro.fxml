<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="introPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.IntroController">
   <children>
      <ImageView id="introImage" fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@/ImagesForView/Introduction.jpg" />
         </image>
      </ImageView>
      <Button fx:id="startGameButton" layoutX="427.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleButtonClick" prefHeight="41.0" prefWidth="111.0" text="Let's Start!" wrapText="true">
         <font>
            <Font name="Arial Unicode MS" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
